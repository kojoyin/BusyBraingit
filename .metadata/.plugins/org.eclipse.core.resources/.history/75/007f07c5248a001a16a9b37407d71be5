package java_practice;

public class ClassWork {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        int sum = 0;
        int count = 1;
        
        for(;count<100; count++) {
        	
        	if(count%5 == 0);
        System.out.println(sum);
        		sum = sum + count;
        	 System.out.println(sum);
          }
	}

}
